{
  "name": "theporadas-site",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:turbo": "next dev --turbopack",
    "build": "node --require ./scripts/node-readlink-patch.cjs ./node_modules/next/dist/bin/next build",
    "build:analyze": "ANALYZE=true npm run build",
    "export": "npm run build && node --require ./scripts/node-readlink-patch.cjs ./node_modules/next/dist/bin/next export",
    "start": "next dev --turbopack",
    "start:prod": "next start",
    "dev:safe": "node --max-old-space-size=4096 ./node_modules/.bin/next dev --turbopack",
    "lint": "eslint .",
    "test:e2e": "playwright test",
    "verify:next-config": "node ./scripts/verify-next-config.js",
    "audit": "lhci autorun --upload.target=temporary-public-storage",
    "postbuild": "next-sitemap",
    "prepare": "husky",
    "gh:status": "powershell -File ../scripts/gh-workflow-status.ps1",
    "gh:logs": "powershell -File ../scripts/gh-workflow-logs.ps1",
    "gh:diagnose": "powershell -File ../scripts/diagnose-ci-failures.ps1",
    "deploy:preview": "powershell -File ../scripts/vercel-deploy-preview.ps1",
    "deploy:prod": "powershell -File ../scripts/vercel-deploy-prod.ps1",
    "deploy:inspect": "powershell -File ../scripts/vercel-inspect.ps1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.2.9",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@supabase/supabase-js": "^2.58.0",
    "browser-image-compression": "^2.0.2",
    "file-saver": "^2.0.5",
    "firebase": "^12.3.0",
    "firebase-admin": "^13.5.0",
    "framer-motion": "^12.23.22",
    "google-auth-library": "^10.4.0",
    "googleapis": "^161.0.0",
    "jszip": "^3.10.1",
    "leaflet": "^1.9.4",
    "next": "^15.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-leaflet": "^5.0.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^15.5.4",
    "@playwright/test": "^1.55.1",
    "@types/node": "^24.6.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vercel/analytics": "^1.5.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.5.4",
    "exifr": "^7.1.3",
    "firebase-tools": "^14.18.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.6",
    "react-masonry-css": "^1.0.16",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "zod": "^3.25.76"
  }
}
